# DEM Backend Environment Configuration Template  
# Copy this file to .env and fill in your specific values for Railway deployment

# Phase 3B.1: Environment detection for production safety checks
APP_ENV=production

# Core Settings
DEFAULT_DEM_ID=s3_au_lidar
USE_S3_SOURCES=true
USE_API_SOURCES=true
CACHE_SIZE_LIMIT=20

# Data Source Configuration
SPATIAL_INDEX_SOURCE=local

# GPXZ.io API Configuration (Optional)
# Get your API key from https://gpxz.io/
GPXZ_API_KEY=your_gpxz_api_key_here
GPXZ_DAILY_LIMIT=100
GPXZ_RATE_LIMIT=1

# Google Elevation API Configuration (Optional)
# Get your API key from Google Cloud Console
GOOGLE_ELEVATION_API_KEY=your_google_api_key_here

# AWS S3 Configuration (Required for S3 sources)
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_S3_BUCKET_NAME=road-engineering-elevation-data
AWS_DEFAULT_REGION=ap-southeast-2

# Server Configuration
CORS_ORIGINS=http://localhost:3001,http://localhost:5173,http://localhost:5174
MAIN_BACKEND_URL=http://localhost:3001

# Logging Configuration
LOG_LEVEL=INFO
SUPPRESS_GDAL_ERRORS=true

# Redis Configuration (Required for production)
# For Railway: Use Railway Redis addon URL
# For local development: redis://localhost:6379 (optional)
REDIS_URL=redis://localhost:6379